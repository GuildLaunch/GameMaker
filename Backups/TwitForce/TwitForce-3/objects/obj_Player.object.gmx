{
    sprite_index = spr_PlayerShielded;
}else{
    sprite_index = spr_Player;
}
if(score&lt;-10)
{
highscore_add("\#TFPilot",score);
room_goto(rm_GameOver);
}

if (levelCounter = 0)
{
    level+=1;
    levelCounter = levelCounterInit;    
}else{
    levelCounter-=1;
}
